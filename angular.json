            "tsConfig": "projects/demo-with-dynamic-form/tsconfig.spec.json",
            "karmaConfig": "projects/demo-with-dynamic-form/karma.conf.js",
            "assets": [
              "projects/demo-with-dynamic-form/src/favicon.ico",
